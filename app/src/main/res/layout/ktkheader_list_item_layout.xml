<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="2dp"
    android:background="@drawable/customgridviewwhite"
    app:cardCornerRadius="3dp">

    <view
        android:id="@+id/viewktk"
        class="androidx.appcompat.widget.AppCompatButton"
        android:layout_width="3dp"
        android:layout_height="0dp"
        android:layout_margin="5dp"
        android:background="#019443"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvdokternyaktk"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="nama dokter"
        android:textColor="#019443"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        app:layout_constraintEnd_toStartOf="@id/TVtitleRatingktk"
        app:layout_constraintStart_toEndOf="@id/viewktk"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/titleTVtgltransaksiktk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="2dp"
        android:text="Pemeriksaan :"
        android:textColor="@color/black"
        android:textSize="8sp"
        app:layout_constraintStart_toEndOf="@id/viewktk"
        app:layout_constraintTop_toBottomOf="@id/tvdokternyaktk"
        tools:ignore="SmallSp" />

    <ImageView
        android:id="@+id/IVdatemasukktk"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:src="@drawable/ic_baseline_date_range_24"
        app:layout_constraintBottom_toBottomOf="@id/titleTVtgltransaksiktk"
        app:layout_constraintStart_toEndOf="@id/titleTVtgltransaksiktk"
        app:layout_constraintTop_toTopOf="@id/titleTVtgltransaksiktk" />

    <TextView
        android:id="@+id/tvtgltransaksiktk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="5dp"
        android:text="Tanggal Pemeriksaan"
        android:textColor="@color/black"
        android:textSize="9sp"
        app:layout_constraintBottom_toBottomOf="@id/IVdatemasukktk"
        app:layout_constraintStart_toEndOf="@id/IVdatemasukktk"
        app:layout_constraintTop_toTopOf="@id/IVdatemasukktk"
        tools:ignore="SmallSp" />

    <TextView
        android:id="@+id/tvtipetransaksiktk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="2dp"
        android:layout_marginRight="5dp"
        android:text="TipeTransaksi"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/viewktk"
        app:layout_constraintTop_toBottomOf="@id/titleTVtgltransaksiktk" />


    <TextView
        android:id="@+id/tvnotransaksiktk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NoRegis"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/tvtipetransaksiktk"
        app:layout_constraintStart_toEndOf="@id/viewktk"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/TVtitleRatingktk"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Beri penilaian :"
        android:textSize="10sp"
        android:layout_marginRight="2dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/IVratingPelayananktk"
        app:layout_constraintBottom_toBottomOf="@id/IVratingPelayananktk"
        app:layout_constraintEnd_toStartOf="@id/IVratingPelayananktk"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/IVratingPelayananktk"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/rating"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/ACBlihatdetailktk"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:background="@drawable/customgridviewtiny"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"
        android:textColor="@color/black"
        android:drawableRight="@drawable/ic_baseline_arrow_right_24"
        android:text="Detail Pemeriksaan"
        android:textSize="10sp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>