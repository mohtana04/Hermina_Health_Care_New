<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    app:cardElevation="10dp"
    app:cardCornerRadius="10dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:padding="5dp">

        <view
            android:id="@+id/viewfar"
            class="androidx.appcompat.widget.AppCompatButton"
            android:layout_width="3dp"
            android:layout_height="0dp"
            android:background="#019443"
            android:layout_margin="5dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/tvdokternyafar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="nama dokter"
            android:textColor="#019443"
            android:textSize="15sp"
            android:textStyle="bold"
            android:layout_marginLeft="5dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@id/viewfar"
            app:layout_constraintEnd_toStartOf="@id/TVtitleRatingfar"/>

        <TextView
            android:id="@+id/titleTVtgltransaksifar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pemeriksaan :"
            android:textColor="@color/black"
            android:textSize="8sp"
            tools:ignore="SmallSp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="2dp"
            app:layout_constraintTop_toBottomOf="@id/tvdokternyafar"
            app:layout_constraintStart_toEndOf="@id/viewfar"/>

        <ImageView
            android:id="@+id/IVdatemasukfar"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:src="@drawable/ic_baseline_date_range_24"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            app:layout_constraintTop_toTopOf="@id/titleTVtgltransaksifar"
            app:layout_constraintBottom_toBottomOf="@id/titleTVtgltransaksifar"
            app:layout_constraintStart_toEndOf="@id/titleTVtgltransaksifar"/>

        <TextView
            android:id="@+id/tvtgltransaksifar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tanggal Pemeriksaan"
            android:textColor="@color/black"
            android:textSize="9sp"
            tools:ignore="SmallSp"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="5dp"
            app:layout_constraintTop_toTopOf="@id/IVdatemasukfar"
            app:layout_constraintBottom_toBottomOf="@id/IVdatemasukfar"
            app:layout_constraintStart_toEndOf="@id/IVdatemasukfar"/>

        <TextView
            android:id="@+id/tvtipetransaksifar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TipeTransaksi"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="2dp"
            app:layout_constraintTop_toBottomOf="@id/titleTVtgltransaksifar"
            app:layout_constraintStart_toEndOf="@id/viewfar"/>

        <TextView
            android:id="@+id/tvnotransaksifar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="NoRegis"
            android:textColor="@color/black"
            android:textSize="12sp"
            android:textStyle="bold"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_marginTop="2dp"
            app:layout_constraintTop_toBottomOf="@id/tvtipetransaksifar"
            app:layout_constraintStart_toEndOf="@id/viewfar"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/TVtitleRatingfar"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Beri penilaian :"
            android:textSize="10sp"
            android:layout_marginRight="2dp"
            android:textColor="@color/black"
            app:layout_constraintTop_toTopOf="@id/IVratingPelayananfar"
            app:layout_constraintBottom_toBottomOf="@id/IVratingPelayananfar"
            app:layout_constraintEnd_toStartOf="@id/IVratingPelayananfar"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/IVratingPelayananfar"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:src="@drawable/rating"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/ACBlihatdetailfar"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:background="@drawable/customgridviewtiny"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="5dp"
            android:textColor="@color/black"
            android:drawableRight="@drawable/ic_baseline_arrow_right_24"
            android:text="Detail Pemeriksaan"
            android:textSize="10sp"
            android:textAllCaps="false"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.cardview.widget.CardView>