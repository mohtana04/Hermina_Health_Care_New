<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_margin="2dp"
    android:background="@drawable/customgridviewwhite"
    app:cardCornerRadius="10dp">

    <view
        android:id="@+id/view1lab"
        class="androidx.appcompat.widget.AppCompatButton"
        android:layout_width="3dp"
        android:layout_height="0dp"
        android:background="#019443"
        android:layout_margin="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/tvdokternyalab"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="nama dokter"
        android:textColor="#019443"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/view1lab"
        app:layout_constraintEnd_toStartOf="@id/TVtitleRating"/>

    <TextView
        android:id="@+id/titleTVtgltransaksilab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pemeriksaan :"
        android:textColor="@color/black"
        android:textSize="8sp"
        tools:ignore="SmallSp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="2dp"
        app:layout_constraintTop_toBottomOf="@id/tvdokternyalab"
        app:layout_constraintStart_toEndOf="@id/view1lab"/>

    <ImageView
        android:id="@+id/IVdatemasuklab"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:src="@drawable/ic_baseline_date_range_24"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toTopOf="@id/titleTVtgltransaksilab"
        app:layout_constraintBottom_toBottomOf="@id/titleTVtgltransaksilab"
        app:layout_constraintStart_toEndOf="@id/titleTVtgltransaksilab"/>

    <TextView
        android:id="@+id/tvtgltransaksilab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tanggal Pemeriksaan"
        android:textColor="@color/black"
        android:textSize="9sp"
        tools:ignore="SmallSp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="5dp"
        app:layout_constraintTop_toTopOf="@id/IVdatemasuklab"
        app:layout_constraintBottom_toBottomOf="@id/IVdatemasuklab"
        app:layout_constraintStart_toEndOf="@id/IVdatemasuklab"/>

    <TextView
        android:id="@+id/tvtipetransaksilab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TipeTransaksi"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/titleTVtgltransaksilab"
        app:layout_constraintStart_toEndOf="@id/view1lab"/>

    <TextView
        android:id="@+id/tvnotransaksilab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NoRegis"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/tvtipetransaksilab"
        app:layout_constraintStart_toEndOf="@id/view1lab"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/TVtitleRating"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Beri penilaian :"
        android:textSize="10sp"
        android:layout_marginRight="2dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/IVratingPelayananlab"
        app:layout_constraintBottom_toBottomOf="@id/IVratingPelayananlab"
        app:layout_constraintEnd_toStartOf="@id/IVratingPelayananlab"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/IVratingPelayananlab"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/rating"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/ACBlihatdetaillab"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:background="@drawable/customgridviewtiny"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"
        android:textColor="@color/black"
        android:drawableRight="@drawable/ic_baseline_arrow_right_24"
        android:text="Detail Pemeriksaan"
        android:textSize="10sp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>