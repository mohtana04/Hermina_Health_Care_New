<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="2dp"
    android:background="@drawable/customgridviewwhite"
    app:cardCornerRadius="10dp">

    <view
        android:id="@+id/viewfis"
        class="androidx.appcompat.widget.AppCompatButton"
        android:layout_width="3dp"
        android:layout_height="0dp"
        android:layout_margin="5dp"
        android:background="#019443"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvdokternyafis"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="nama dokter"
        android:textColor="#019443"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        app:layout_constraintEnd_toStartOf="@id/TVtitleRatingfis"
        app:layout_constraintStart_toEndOf="@id/viewfis"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/titleTVtgltransaksifis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="2dp"
        android:text="Pemeriksaan :"
        android:textColor="@color/black"
        android:textSize="8sp"
        app:layout_constraintStart_toEndOf="@id/viewfis"
        app:layout_constraintTop_toBottomOf="@id/tvdokternyafis"
        tools:ignore="SmallSp" />

    <ImageView
        android:id="@+id/IVdatemasukfis"
        android:layout_width="10dp"
        android:layout_height="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:src="@drawable/ic_baseline_date_range_24"
        app:layout_constraintBottom_toBottomOf="@id/titleTVtgltransaksifis"
        app:layout_constraintStart_toEndOf="@id/titleTVtgltransaksifis"
        app:layout_constraintTop_toTopOf="@id/titleTVtgltransaksifis" />

    <TextView
        android:id="@+id/tvtgltransaksifis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="5dp"
        android:text="Tanggal Pemeriksaan"
        android:textColor="@color/black"
        android:textSize="9sp"
        app:layout_constraintBottom_toBottomOf="@id/IVdatemasukfis"
        app:layout_constraintStart_toEndOf="@id/IVdatemasukfis"
        app:layout_constraintTop_toTopOf="@id/IVdatemasukfis"
        tools:ignore="SmallSp" />

    <TextView
        android:id="@+id/tvtipetransaksifis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="2dp"
        android:layout_marginRight="5dp"
        android:text="TipeTransaksi"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/viewfis"
        app:layout_constraintTop_toBottomOf="@id/titleTVtgltransaksifis" />


    <TextView
        android:id="@+id/tvnotransaksifis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NoRegis"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/tvtipetransaksifis"
        app:layout_constraintStart_toEndOf="@id/viewfis"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/TVtitleRatingfis"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Beri penilaian :"
        android:textSize="10sp"
        android:layout_marginRight="2dp"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/IVratingPelayananfis"
        app:layout_constraintBottom_toBottomOf="@id/IVratingPelayananfis"
        app:layout_constraintEnd_toStartOf="@id/IVratingPelayananfis"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/IVratingPelayananfis"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/rating"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/ACBlihatdetailfis"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:background="@drawable/customgridviewtiny"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="5dp"
        android:textColor="@color/black"
        android:drawableRight="@drawable/ic_baseline_arrow_right_24"
        android:text="Detail Pemeriksaan"
        android:textSize="10sp"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>