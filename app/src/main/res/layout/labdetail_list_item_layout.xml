<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp">

    <view
        android:id="@+id/view1labdet"
        class="androidx.appcompat.widget.AppCompatButton"
        android:layout_width="3dp"
        android:layout_height="0dp"
        android:background="#019443"
        android:layout_margin="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/TVtestindonesialabdet"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/TVkelompoknamalabdet"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="nama Pemeriksaan"
        android:textColor="#019443"
        android:textSize="15sp"
        android:textStyle="bold"
        android:layout_marginLeft="5dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/view1labdet"/>

    <TextView
        android:id="@+id/TVtestindonesialabdet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:textColor="@color/black"
        android:scaleType="centerCrop"
        android:text="Pemeriksaansub"
        android:layout_marginLeft="5dp"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@id/view1labdet"
        app:layout_constraintTop_toBottomOf="@id/TVkelompoknamalabdet"/>

    <TextView
        android:id="@+id/TVtitlehasilkarakterlabdet"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="Hasil"
        android:textColor="@color/black"
        android:textSize="8sp"
        tools:ignore="SmallSp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="2dp"
        app:layout_constraintTop_toBottomOf="@id/TVtestindonesialabdet"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/TVtitlenormalkarakterlabdet"/>

    <TextView
        android:id="@+id/TVhasillabdet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:textColor="@color/black"
        android:text="Nilai Normal"
        android:textStyle="bold"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@id/TVtitlehasilkarakterlabdet"
        app:layout_constraintEnd_toEndOf="@id/TVtitlehasilkarakterlabdet"
        app:layout_constraintStart_toStartOf="@id/TVtitlehasilkarakterlabdet"/>

    <TextView
        android:id="@+id/TVtitlenormalkarakterlabdet"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Nilai Normal"
        android:gravity="center_horizontal"
        android:textColor="@color/black"
        android:textSize="8sp"
        tools:ignore="SmallSp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="2dp"
        app:layout_constraintTop_toBottomOf="@id/TVtestindonesialabdet"
        app:layout_constraintStart_toEndOf="@id/TVtitlehasilkarakterlabdet"
        app:layout_constraintEnd_toStartOf="@id/TVtitleSatuanlabdet"/>

    <TextView
        android:id="@+id/TVnormalkarakterlabdet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:text="Normal Karakter"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@id/TVtitlenormalkarakterlabdet"
        app:layout_constraintStart_toStartOf="@id/TVtitlenormalkarakterlabdet"
        app:layout_constraintEnd_toEndOf="@id/TVtitlenormalkarakterlabdet"/>

    <TextView
        android:id="@+id/TVtitleSatuanlabdet"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Satuan"
        android:textColor="@color/black"
        android:gravity="center_horizontal"
        android:textSize="8sp"
        tools:ignore="SmallSp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="2dp"
        app:layout_constraintTop_toBottomOf="@id/TVtestindonesialabdet"
        app:layout_constraintStart_toEndOf="@id/TVtitlenormalkarakterlabdet"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/TVSatuanlabdet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="3dp"
        android:textColor="@color/black"
        android:text="Satuan"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/TVtitleSatuanlabdet"
        app:layout_constraintStart_toStartOf="@id/TVtitleSatuanlabdet"
        app:layout_constraintEnd_toEndOf="@id/TVtitleSatuanlabdet"/>

</androidx.constraintlayout.widget.ConstraintLayout>